<?xml version="1.0"?>

<dbp:module
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:dbp="http://dbpager.org.ru/schemas/dbp/3.0"
	xmlns:lib="http://dbpager.org.ru/schemas/dbp/3.0/custom"
	xmlns:app="http://dbpager.org.ru/schemas/dbp/3.0/custom"
>

<dbp:tag name="categories">
	<!-- Blog Categories Well -->
	<div class="well">
		<h4>Blog Categories</h4>
		<div class="row">
			<div class="col-lg-12">
				<ul class="list-unstyled" style="column-count:2;-moz-column-count:2;-webkit-column-count:2">
					<dbp:execute />
				</ul>
			</div>
		</div>
		<!-- /.row -->
	</div>
</dbp:tag>

</dbp:module>
