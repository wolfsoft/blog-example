<?xml version="1.0"?>

<dbp:module
	xml:lang="en"
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:dbp="http://dbpager.org.ru/schemas/dbp/3.0"
	xmlns:lib="http://dbpager.org.ru/schemas/dbp/3.0/custom"
	xmlns:app="http://dbpager.org.ru/schemas/dbp/3.0/custom"
	xmlns:its="http://www.w3.org/2005/11/its" 
	its:version="2.0"
	its:translate="no"
>

<dbp:tag name="page">

<app:html>

	<app:navigation />

	<!-- Page Content -->
	<div class="container">

		<div class="row">
			<main class="col-md-8">
				<dbp:execute />
			</main>
			<aside class="col-md-4">
				<app:sidebar />
			</aside>
		</div>

		<hr />

		<!-- Footer -->
		<footer>
			<app:footer />
		</footer>

	</div>

</app:html>

</dbp:tag>

</dbp:module>
