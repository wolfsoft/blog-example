<?xml version="1.0"?>

<dbp:module
	xml:lang="en"
	xml:space="preserve"
	xmlns:its="http://www.w3.org/2005/11/its" 
	its:version="1.0"
	its:translate="no"
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:dbp="http://dbpager.org.ru/schemas/dbp/3.0"
	xmlns:lib="http://dbpager.org.ru/schemas/dbp/3.0/custom"
	xmlns:app="http://dbpager.org.ru/schemas/dbp/3.0/custom"
>

<dbp:tag name="admin-page">

<dbp:set name="content_type">text/html</dbp:set>

&lt;!DOCTYPE html&gt;
<html lang="en">

<head>
	<meta charset="utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<lib:default name="title" value="Administrative Section">
		<title>{$title}</title>
	</lib:default>
	<lib:default name="description" value="Administrative section of the blog">
		<meta name="description" content="{$description}" />
	</lib:default>

	<link href="/assets/stylesheets/backend.css" rel="stylesheet" />

	<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
	&lt;!--[if lt IE 9]&gt;
		<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
		<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
	&lt;![endif]--&gt;
</head>

<body>
	<app:navigation />

	<!-- Page Content -->
	<div class="container">

		<div class="row">

			<!-- Blog Entries Column -->
			<div class="col-md-9">
				<dbp:execute />
			</div>

			<!-- Blog Sidebar Widgets Column -->
			<div class="col-md-3">
				<app:admin-view-sidebar />
			</div>

		</div>
		<!-- /.row -->

		<hr />

		<!-- Footer -->
		<footer>
			<div class="row">
				<div class="col-lg-12">
					<p its:translate="yes">Copyright Â© 2015 Dennis Prochko</p>
				</div>
				<!-- /.col-lg-12 -->
			</div>
			<!-- /.row -->
		</footer>

	</div>
	<!-- /.container -->

	<!-- Bootstrap Core JavaScript -->
	<script src="/assets/javascript/backend.min.js"></script>

</body>

</html>

</dbp:tag>

</dbp:module>
